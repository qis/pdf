@PACKAGE_INIT@
include(CMakeFindDependencyMacro)

if(NOT WIN32)
  find_dependency(OpenSSL REQUIRED)
endif()

find_dependency(BZip2 REQUIRED)
find_dependency(LibLZMA REQUIRED)
find_dependency(Freetype REQUIRED)
find_dependency(JPEG REQUIRED)
